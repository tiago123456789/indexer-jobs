<% include ../partials/header %>
    <div class="container">
        <h1 class="text-center">Datas user</h1>
        <div class="row">
            <a href="/" style="margin-left: 14%;" class=" btn  btn-danger">Voltar</a>
        </div>
        <br/>
        <div class="row">
            <form class="col-md-9" style="margin: 0px auto;">
                <div class="form-group">
                    <label for="">Username:</label>
                    <input type="text" name="username" value="<%= user.username%>" class="form-control" />
                </div>

                <div class="form-group">
                    <label for="">Cellphone:</label>
                    <input type="tel" name="cellphone" value="<%= user.cellphone%>" class="form-control" />
                </div>

                <div class="form-group">
                    <label for="">Email:</label>
                    <input type="email" name="email" value="<%= user.email%>" class="form-control" />
                </div>
            </form>
        </div>
    </div>
<% include ../partials/footer %>